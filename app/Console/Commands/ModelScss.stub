$elements: (
        GetBtn,
        GetAllBtn,
        CreateBtn,
        UpdateBtn,
        DeleteBtn,
        GetFrm,
        GetAllFrm,
        CreateFrm,
        UpdateFrm,
        DeleteFrm,
        GetMdl,
        GetAllMdl,
        CreateMdl,
        UpdateMdl,
        DeleteMdl,
        GetMdlBtn,
        GetAllMdlBtn,
        CreateMdlBtn,
        UpdateMdlBtn,
        DeleteMdlBtn);

@each $element in $elements {
  .{{ NAMECLASS }}#{$element} {
    font-family: inherit;
  }
}
